    <%= f.text_field :keytwo %><br>
    <%= f.label :keythree %><br>
    <%= f.text_field :keythree %><br>
    <%= f.label :keyfour %><br>
    <%= f.text_field :keyfour %><br>
    <%= f.label :keyfive %><br>
    <%= f.text_field :keyfive %><br>
